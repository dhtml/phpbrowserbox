        "electron-webpack": "^2.8.2",
        "esbuild-jest": "^0.5.0",
        "eslint": "^8.7.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.5.0",
        "prettier": "^2.5.1",
        "ts-node": "^10.9.1",
