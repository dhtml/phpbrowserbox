{
  "name": "phpbrowserbox",
  "version": "5.0.0",
  "description": "Run PHP Application As Windows Desktop App",
  "main": "./out/main/index.js",
  "author": "Anthony Ogundipe / diltony@gmail.com / https://anthonyogundipe.com",
  "homepage": "https://phpbrowserbox.com",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck": "npm run typecheck:node",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:win:dist": "npm run build && electron-builder --win --config && cmd.exe /c deploy.bat",
    "build:copy": "cmd.exe /c deploy.bat",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.2",
    "@electron-toolkit/utils": "^1.0.2",
    "electron-config": "^2.0.0",
    "request": "^2.88.2",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.3",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/node": "16.11.22",
    "electron": "^21.3.0",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.14",
    "eslint": "^8.28.0",
    "less": "^4.1.3",
    "prettier": "^2.7.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
}
